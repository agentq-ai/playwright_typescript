{
    "compilerOptions": 
    {
      "target": "ES6",
      "lib": ["esnext"],
      "preserveConstEnums": true,
      "moduleResolution": "node",
      "strict": true,
      "sourceMap": true,
      "module": "esnext",
      "types": [
        "node"
      ],
      "resolveJsonModule": true,
      "esModuleInterop": true
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
}
  